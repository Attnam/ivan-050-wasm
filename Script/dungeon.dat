			GenerateFountains=true;
			AllowLockedDoors=false;
			AllowBoobyTrappedDoors=false;
		}
	}

	Level 0;
	{
		Square, BoundedRandom 1, 1, 19, 19, NOTWALKABLE|ATTACHABLE;
