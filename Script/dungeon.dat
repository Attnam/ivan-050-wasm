Dungeon UNDERWATERTUNNEL;
{
	Levels=3;

	LevelDefault
	{
		FillSquare=solidterrain(GROUND),wall(EARTH);

		Variable XSize = 80;
		Variable YSize = 20;

		Size=XSize, YSize;
		Rooms=5+rand%6;
		Items=15+rand%16;
		GenerateMonsters=true;
		ReCalculate=true;
		OnGround=false;
		TeamDefault=1;
		AmbientLight=0;
		LOSModifier=16;
		IgnoreDefaultSpecialSquares = false;

		Square, Random;
		{
			Item = beartrap { Team = MONSTER_TEAM; Active = true; }
			Times = rand%2;
		}

		RoomDefault
		{
			Pos=2+rand%(XSize-6),2+rand%(YSize-6);
			Size=4+rand%8,4+rand%8;
			AltarPossible=false;
